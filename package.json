{
  "name": "react-walkthrough",
  "version": "1.0.5",
  "description": "Simple declarative user walkthroughs and onboarding with React",
  "main": "lib/index.js",
  "repository": "izaaksultan/react-walkthrough",
  "scripts": {
    "test": "jest --no-cache --coverage",
    "storybook": "start-storybook -p 9001",
    "build": "webpack --config webpack.config.js --progress --colors",
    "build-storybook": "build-storybook",
    "deploy-gh-pages": "build-storybook && git checkout gh-pages && rm -rf static && mv ./storybook-static/* ."
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-test-renderer",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/immutable",
      "<rootDir>/node_modules/jsdom"
    ],
    "moduleNameMapper": {
      "^.*[.](scss|SCSS|css|CSS)$": "<rootDir>/jest/CSSModule.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.setup.js"
    ]
  },
  "author": "Izaak Sultan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@kadira/storybook": "^2.3.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "css-loader": "^3.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "immutable": "^3.8.1",
    "jest-cli": "^24.0.0",
    "jsdom": "^16.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.0.0",
    "prop-types": "^15.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.8.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "guid": "0.0.12"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "prop-types": "^15.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
